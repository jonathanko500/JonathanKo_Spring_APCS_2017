package textExcel;

public class RealCell implements Cell {
	private String command;
	public RealCell(String input){
		command=input;
	}
	public String abbreviatedCellText() {
		String shortInput="";
		if(shortInput.length()<10){
			while (shortInput.length()<10){
				shortInput += " ";
			}
			return shortInput;
		}
		if(shortInput.length()>10){
			shortInput=shortInput.substring(0,10);
		}
		
		return shortInput;
	}

	@Override
	public String fullCellText() {
		return command;
	}

}
